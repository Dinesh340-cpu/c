//Program for swapping of values using call by value
 #include<stdio.h>
 void main()
   {
      int a,b;
      void swap(int x,int y);
      printf("\nEnter the values of a and b:");
      scanf("%d%d",&a,&b);
      swap(a,b);
      printf(("\n In main ()a=%d ,b=%d",a,b);
    }
    void swap(int x,int y)
    {
        int t;
        t=x;
        x=y;
        y=t;
        printf("\n In swapping a=%d b=%d",x,y);
     }
     
